  "license": "GPL-3.0+",
  "scripts": {
    "wp:version": "docker-compose run --rm wordpress-cli wp core version --user=1",
    "wp:version-run": "node helpers/childVars/getWpVersion.js",
    "wc:version-run": "node helpers/childVars/getWcVersion.js",
    "klarna:version-run": "node helpers/childVars/getKlarnaVersion.js",
    "wp:php-version-run": "node helpers/childVars/getPhpVersion.js",
    "lint": "npx eslint --ext .js ./ --fix",
    "test": "jest",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down -v",
    "docker:restart": "npm run docker:down && npm run docker:up",
    "docker:setup": "docker-compose run --rm wordpress-cli ./wp-content/plugins/klarna-checkout-for-woocommerce/tests/e2e/bin/setup.sh",
    "docker:import-data": "docker-compose run --rm wordpress-cli ./wp-content/plugins/klarna-checkout-for-woocommerce/tests/e2e/bin/import.sh",
    "docker:site-init": "npm run docker:setup && npm run docker:import-data",
    "jettison": "jest --detectOpenHandles --outputFile=./outputs/output.txt",
    "wp:install": "node ./cli/install.js",
    "e2e:test": "yarn docker:up && yarn wp:install && yarn jettison && yarn docker:down"
  },
  "version": "1.0.0"
}
